<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lotlockbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.login.AdminLogin"
            android:parentActivityName=".MainActivity"
            >
        </activity>
        <activity android:name=".ui.login.LoggedInUserView"
        android:parentActivityName=".ui.login.AdminLogin"
        >
        </activity>
        <activity android:name=".sendPackage"
            android:parentActivityName=".ui.login.LoggedInUserView"
            >
        </activity>
        <activity android:name=".packageSent"
            android:parentActivityName=".sendPackage"
            >
        </activity>

        <activity android:name=".adminSentHistory"
            android:parentActivityName=".ui.login.LoggedInUserView"
            >
        </activity>

        <activity android:name=".settingPage"
            android:parentActivityName=".ui.login.LoggedInUserView"
            >
        </activity>

    </application>

</manifest>