<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.lotlockbox">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".create_account"></activity>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".packageSent"
            android:parentActivityName=".sendPackage"></activity>
        <activity
            android:name=".adminSentHistory"
            android:parentActivityName=".ui.login.LoggedInUserView"></activity>
        <activity
            android:name=".settingPage"
            android:parentActivityName=".ui.login.LoggedInUserView"></activity>
        <activity
            android:name=".ui.login.AdminLogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.login.LoggedInUserView"
            android:parentActivityName=".ui.login.AdminLogin" />
        <activity
            android:name=".sendPackage"
            android:parentActivityName=".ui.login.LoggedInUserView" />
        <activity
            android:name=".ui.login.CustomerLogin"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".ui.login.Cus_LoggedInUserView"
            android:parentActivityName=".ui.login.CustomerLogin" />
        <activity
            android:name=".pickupResult"
            android:parentActivityName=".ui.login.Cus_LoggedInUserView" />
        <activity
            android:name=".customerHistory"
            android:parentActivityName=".ui.login.Cus_LoggedInUserView" />

    </application>

</manifest>